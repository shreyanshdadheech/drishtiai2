{
  "name": "drishti-ai",
  "productName": "Drishti AI",
  "version": "1.0.2",
  "description": "Retina AI Image Analysis Tool",
  "main": ".vite/build/main.js",
  "private": true,
  "repository": "https://github.com/shreyanshdadheech/drishtiai2",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "scripts": {
    "windows": "npx electron-packager . drishti  --out=dist --overwrite --icon=icon.ico",
    "dist": "electron-builder",
    "build:win":"electron-builder --win",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make --platform=win32 --arch=x64",
    "publish": "electron-forge publish --platform=win32 --arch=x64",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:all": "vitest run && playwright test",
    "postinstall": "electron-builder install-app-deps",
    "generate-latest": "node scripts/generate-latest-yml.js"
  },
  "build": {
    "appId": "com.nodence.drishtiai",
    "productName": "Drishti AI",
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "publish": {
      "provider": "github",
      "owner": "shreyanshdadheech",
      "repo": "drishtiai2",
      "private": false,
      "releaseType": "release"
    }
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "author": "NodenceX",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
