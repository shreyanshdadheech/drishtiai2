    "@electron/fuses": "^1.8.0",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.50.1",
    "@tailwindcss/postcss": "^4.0.3",
    "@testing-library/jest-dom": "^6.6.3",
